public class LambdaExpressions {
    interface MyOp {
    	double operation(double a, double b);
    }
    
    public static exe(double a, double b, MyOp operation);
	
	public static void main(String[] args) {
        MyOp div = (a,b) -> a/b;
        System.out.println(div.operation(3, 4));
 	}

}
