class A {
	protected A get() {
		System.out.println("Class A");
		return null;
	}
}

class B extends A {
	
	@Override
	public B get() {
		this();
		System.out.println("Class B");
		
		return null;
	}
}
public class Example {
     public static final void main(String[] args) {
    	 A a = new B();
    	 if(a instanceof B) {
    	   B b = (B)a;
    	   
    	 }
     }
}
